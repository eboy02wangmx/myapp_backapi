    <select id="findCountByEntryNo" parameterType="jp.co.myapp.api.app.request.EntryRegistrationRequest" resultType="int">
    <![CDATA[
        SELECT
        	count(1) AS cnt
        FROM
            ENTRY_HA
        WHERE
        	1 = 1
    ]]>
